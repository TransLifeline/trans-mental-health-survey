header
  a(href='http://www.thetaskforce.org').logo: img(src='images/logo1.png')
  a(href='http://www.translifeline.org').logo.fa-pull-right: img(src='images/logo2.png')
  if menu.length > 0
    i.fa.fa-bars.fa-pull-right#menu-button
    nav
      i.fa.fa-bars.fa-pull-right
      ul.menu
        each item in menu
          li
            if item.url
              a(href=item.url)=item.label
            else if item.submenu
              span=item.label
                i.fa.fa-caret-down
              ul.submenu.submenu-collapsed
                each subitem in item.submenu
                  li
                    if subitem.url
                      a(href=subitem.url)=subitem.label
                    else
                      span=subitem.label
